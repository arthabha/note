{
  "title": "个人笔记",
  "description": "记录每天的点点滴滴",
  "author": "loveagri",
  "output.name": "site",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "root": "./",
  "structure": {
    "readme": "README.md"
  },
  "plugins": [
    "-lunr",
    "-search",
    "-highlight",
    "-livereload",
    "toggle-chapters",
    "search-plus@^0.0.11",
    "diy-introduction",
    "simple-page-toc@^0.1.1",
    "github@^2.0.0",
    "accordion",
    "anchor-navigation-ex",
    "github-buttons@2.1.0",
    "edit-link@^2.0.2",
    "disqus@^0.1.0",
    "prism-themes@^0.0.2",
    "advanced-emoji@^0.2.1",
    "anchors@^0.7.1",
    "include-codeblock@^3.0.2",
    "ace@^0.3.2",
    "emphasize@^1.1.0",
    "katex@^1.1.3",
    "splitter@^0.0.8",
    "mermaid-gb3@2.1.0",
    "tbfed-pagefooter@^0.0.1",
    "expandable-chapters-small@^0.1.7",
    "sectionx",
    "popup",
    "prism",
    "-highlight",
    "lightbox",
    "hide-element",
    "donate@^1.0.2",
    "local-video@^1.0.1",
    "sitemap-general@^0.1.1",
    "favicon@^0.0.2",
    "todo@^0.1.3",
    "terminal@^0.3.2",
    "alerts@^0.2.0",
    "flexible-alerts",
    "pageview-count",
    "include-csv@^0.1.0",
    "puml@^1.0.1",
    "musicxml@^1.0.2",
    "klipse@^1.2.0",
    "versions-select@^0.1.1",
    "rss@^3.0.2",
    "page-copyright",
    "-sharing",
    "code",
    "sharing-plus@^0.0.2",
    "graph@^0.1.0",
    "copy-code-button",
    "change_girls",
    "expandable-chapters",
    "chapter-fold",
    "-lunr",
    "-search",
    "search-pro",
    "chart@^0.2.0"
  ],
  "pluginsConfig": {
    "change_girls" : {
      "time" : 10,
      "urls" : [
        "girlUrl1", "girlUrl2",...""
      ]
    }
    "code": {
      "copyButtons": false
    },
    "theme-default": {
      "showLevel": false
    },
    "hide-element": {
      "elements": [
        ".gitbook-link",
        "span.copyright",
        ".copyright",
        ".book-anchor",
        ".disqus-footer",
        ".disqus-footer__wrapper",
        ".page-footer .copyright"
      ]
    },
    "introduction-text": "封面",
    "disqus": {
      "shortName": "loveagri"
    },
    "github": {
      "url": "https://github.com/loveagri/note"
    },
    "github-buttons": {
      "repo": "loveagri/note",
      "types": [
        "star"
      ],
      "size": "small"
    },
    "include-codeblock": {
      "template": "ace",
      "unindent": true,
      "edit": true
    },
    "sharing": {
      "douban": false,
      "facebook": false,
      "google": true,
      "hatenaBookmark": false,
      "instapaper": false,
      "line": true,
      "linkedin": true,
      "messenger": false,
      "pocket": false,
      "qq": false,
      "qzone": true,
      "stumbleupon": false,
      "twitter": false,
      "viber": false,
      "vk": false,
      "weibo": true,
      "whatsapp": false,
      "all": [
        "douban", "facebook", "google", "hatenaBookmark",
        "instapaper", "linkedin","twitter", "weibo",
        "messenger","qq", "qzone","viber","vk","weibo",
        "pocket", "stumbleupon","whatsapp"
      ]
    },
    "tbfed-pagefooter": {
      "copyright": "每天记录一点点，每天提高一点点",
      "modify_label": "网站备案信息：冀ICP备19024518号-1       ",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "page-copyright": {
      "description": "modified at",
      "signature": "你的签名",
      "wisdom": "Designer, Frontend Developer & overall web enthusiast",
      "format": "YYYY-MM-dd hh:mm:ss",
      "copyright": "Copyright &#169; 你的名字",
      "timeColor": "#666",
      "copyrightColor": "#666",
      "utcOffset": "8",
      "style": "normal",
      "noPowered": true
    },
    "donate": {
      "wechat": "https://raw.githubusercontent.com/loveagri/note/master/ud-img/wechat.jpeg",
      "alipay": "https://raw.githubusercontent.com/loveagri/note/master/ud-img/alipay.jpeg",
      "title": "",
      "button": "打赏",
      "wechatText": ""
    },
    "simple-page-toc": {
      "maxDepth": 3,
      "skipFirstH1": true
    },
    "edit-link": {
      "base": "https://github.com/loveagri/note/blob/master/content",
      "label": "Edit This Page"
    },
    "sitemap-general": {
      "prefix": "https://loveagri.github.io/note/"
    },
    "sectionx": {
      "tag": "b"
    },
    "favicon": {
      "shortcut": "favicon.ico",
      "bookmark": "favicon.ico"
    },
    "terminal": {
      "copyButtons": true,
      "fade": false,
      "style": "flat"
    },
    "rss": {
      "title": "个人笔记",
      "description": "记录每天的点点滴滴",
      "author": "loveagri",
      "feed_url": "https://loveagri.github.io/note/",
      "site_url": "https://loveagri.github.io/note/",
      "managingEditor": "1044531744@qq.com",
      "webMaster": "1044531744@qq.com",
      "categories": [
        "gitbook"
      ]
    }
  }
}
